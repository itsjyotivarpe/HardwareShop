create database asedata;
use asedata;

create table member
(id  VARCHAR(8) PRIMARY KEY,
password VARCHAR(20),
name VARCHAR(30),
phone INT(11));

insert into member values(23451,'love','laddu',988627588);

create table userdetails
(id VARCHAR(8) PRIMARY KEY,
firstname VARCHAR(20),
email VARCHAR(20),
address VARCHAR(20),
city VARCHAR(20),
state VARCHAR(20),
zip  VARCHAR(20),
cardname VARCHAR(20),
cardnumber VARCHAR(20),
expmonth VARCHAR(20),
expyear VARCHAR(20),
cvv	 VARCHAR(20));
		